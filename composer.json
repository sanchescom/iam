{
    "name": "thiagovictorino/iam",
    "description": "Identity and Access Management based on IAM AWS service concepts",
    "type": "library",
    "require": {
        "laravel/framework": "^5.5",
        "firebase/php-jwt": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thiago Victorino",
            "email": "eu@thiagovictorino.com"
        }
    ],
    "minimum-stability": "alpha",
    "autoload": {
        "psr-4": {
            "thiagovictorino\\IAM\\": "src/",
            "thiagovictorino\\IAM\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "thiagovictorino\\IAM\\IAMServiceProvider"
            ],
            "aliases": {
                "IAM": "thiagovictorino\\IAM\\Facade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
